<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
  <appSettings>

    <!--NOTE : This is the entity use for threading process
    only one entity wll take
    need separate configuration for separate entities-->

    <add key="ENTITY" value="82-denmg"/>
    <add key="Server" value="localhost/apiduo"/>
    <add key="appserver" value="172.16.8.85"/>
    <add key="username" value="den"/>
    <add key="password" value="password@123"/>
    <add key="Threads" value="4"/>
    <!--Just to Run Passing Aguments 0 Will Run As a Service 1 /-->
    <add key="Service" value="0"/>
    <!--skip already created one 0 run and update the file 1 /-->
    <add key="skip" value="0"/>
    <!--Call cache will call cached values update will update the cache /-->
    <add key="call" value="update"/>
    <!--Record count to selet both addition and removal together /-->
    <add key="folder" value="\var\media\"/>
    <add key="RecordCount_recharge" value="0"/>
    <add key="RecordCount_reconnect" value="0"/>
    <add key="RecordCount_disconnect" value="0"/>

    <!-- No of threads /-->
    <add key="ThreadCount" value="3"/>



  </appSettings>
</configuration>
